      "content": "\"use client\";\n\nimport {\n  Sidebar,\n  SidebarContent,\n  SidebarGroup,\n  SidebarGroupContent,\n  SidebarGroupLabel,\n  SidebarHeader,\n  SidebarMenu,\n  SidebarMenuItem,\n  SidebarMenuButton,\n  SidebarProvider,\n  SidebarTrigger,\n  SidebarFooter,\n} from \"@/components/ui/sidebar\";\nimport {\n  Home,\n  Inbox,\n  Calendar,\n  Settings,\n  Users,\n  FolderKanban,\n  ChevronDown,\n  Plus\n} from \"lucide-react\";\nimport {\n  DropdownMenu,\n  DropdownMenuContent,\n  DropdownMenuItem,\n  DropdownMenuTrigger,\n} from \"@/components/ui/dropdown-menu\";\n\nconst SidebarWithTeams = () => {\n  return (\n    <div className=\"flex h-full w-full min-h-[600px]\">\n      <SidebarProvider defaultOpen={true}>\n        <Sidebar>\n          <SidebarHeader className=\"border-b p-4\">\n            <DropdownMenu>\n              <DropdownMenuTrigger className=\"flex w-full items-center gap-2 rounded-lg px-2 py-1.5 text-left hover:bg-accent\">\n                <div className=\"flex h-8 w-8 items-center justify-center rounded-lg bg-primary text-primary-foreground\">\n                  <Users className=\"h-4 w-4\" />\n                </div>\n                <div className=\"flex-1 overflow-hidden\">\n                  <p className=\"truncate text-sm font-medium\">Acme Inc</p>\n                  <p className=\"truncate text-xs text-muted-foreground\">Free Plan</p>\n                </div>\n                <ChevronDown className=\"h-4 w-4 text-muted-foreground\" />\n              </DropdownMenuTrigger>\n              <DropdownMenuContent align=\"start\" className=\"w-[240px]\">\n                <DropdownMenuItem>\n                  <Users className=\"mr-2 h-4 w-4\" />\n                  <span>Acme Inc</span>\n                </DropdownMenuItem>\n                <DropdownMenuItem>\n                  <Users className=\"mr-2 h-4 w-4\" />\n                  <span>Acme Corp</span>\n                </DropdownMenuItem>\n                <DropdownMenuItem>\n                  <Plus className=\"mr-2 h-4 w-4\" />\n                  <span>Create team</span>\n                </DropdownMenuItem>\n              </DropdownMenuContent>\n            </DropdownMenu>\n          </SidebarHeader>\n          <SidebarContent>\n            <SidebarGroup>\n              <SidebarGroupLabel>Navigation</SidebarGroupLabel>\n              <SidebarGroupContent>\n                <SidebarMenu>\n                  <SidebarMenuItem>\n                    <SidebarMenuButton>\n                      <Home className=\"h-4 w-4\" />\n                      <span>Home</span>\n                    </SidebarMenuButton>\n                  </SidebarMenuItem>\n                  <SidebarMenuItem>\n                    <SidebarMenuButton>\n                      <Inbox className=\"h-4 w-4\" />\n                      <span>Inbox</span>\n                    </SidebarMenuButton>\n                  </SidebarMenuItem>\n                  <SidebarMenuItem>\n                    <SidebarMenuButton>\n                      <Calendar className=\"h-4 w-4\" />\n                      <span>Calendar</span>\n                    </SidebarMenuButton>\n                  </SidebarMenuItem>\n                  <SidebarMenuItem>\n                    <SidebarMenuButton>\n                      <FolderKanban className=\"h-4 w-4\" />\n                      <span>Projects</span>\n                    </SidebarMenuButton>\n                  </SidebarMenuItem>\n                </SidebarMenu>\n              </SidebarGroupContent>\n            </SidebarGroup>\n            <SidebarGroup>\n              <SidebarGroupLabel>Settings</SidebarGroupLabel>\n              <SidebarGroupContent>\n                <SidebarMenu>\n                  <SidebarMenuItem>\n                    <SidebarMenuButton>\n                      <Settings className=\"h-4 w-4\" />\n                      <span>General</span>\n                    </SidebarMenuButton>\n                  </SidebarMenuItem>\n                  <SidebarMenuItem>\n                    <SidebarMenuButton>\n                      <Users className=\"h-4 w-4\" />\n                      <span>Team</span>\n                    </SidebarMenuButton>\n                  </SidebarMenuItem>\n                </SidebarMenu>\n              </SidebarGroupContent>\n            </SidebarGroup>\n          </SidebarContent>\n          <SidebarFooter className=\"border-t p-4\">\n            <div className=\"flex items-center gap-2 rounded-lg border bg-muted/50 px-3 py-2\">\n              <div className=\"flex h-8 w-8 items-center justify-center rounded-full bg-primary text-primary-foreground text-xs font-medium\">\n                JD\n              </div>\n              <div className=\"flex-1 overflow-hidden\">\n                <p className=\"truncate text-sm font-medium\">John Doe</p>\n                <p className=\"truncate text-xs text-muted-foreground\">john@example.com</p>\n              </div>\n            </div>\n          </SidebarFooter>\n        </Sidebar>\n        <main className=\"flex-1 overflow-auto\">\n          <header className=\"flex h-16 items-center gap-4 border-b px-6\">\n            <SidebarTrigger />\n            <h1 className=\"text-xl font-semibold\">Team Dashboard</h1>\n          </header>\n          <div className=\"p-6\">\n            <div className=\"space-y-4\">\n              <div className=\"rounded-lg border bg-card p-6\">\n                <h3 className=\"font-semibold mb-4\">Team Overview</h3>\n                <p className=\"text-sm text-muted-foreground\">\n                  Manage your team members, projects, and workspace settings from this dashboard.\n                </p>\n              </div>\n            </div>\n          </div>\n        </main>\n      </SidebarProvider>\n    </div>\n  );\n};\n\nexport { SidebarWithTeams }",
